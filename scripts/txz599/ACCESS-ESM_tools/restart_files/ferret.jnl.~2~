use co2_3d_old.nc
use co2_3d_new.nc
use co2_3d_new_essential.nc
show d
shade field1564[k=1,d=1]-field1564[k=1,d=3]
shade field1564[k=1,d=1]-field1564[k=1,d=2]
shade field1564[k=1,d=2]-field1564[k=1,d=3]
shade field1564[k=38,d=2]-field1564[k=38,d=3]
shade field1564[k=38,d=1]-field1564[k=38,d=3]
exit
