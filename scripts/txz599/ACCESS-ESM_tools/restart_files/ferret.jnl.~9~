shade field1391[k=2,d=1] - field1391[k=2,d=2]
shade field1391[k=3,d=1] - field1391[k=3,d=2]
shade field1391[k=4,d=1] - field1391[k=4,d=2]
shade field1391[k=5,d=1] - field1391[k=5,d=2]
shade field1391[k=6,d=1] - field1391[k=6,d=2]
shade field1391[k=7,d=1] - field1391[k=7,d=2]
shade field1391[k=8,d=1] - field1391[k=8,d=2]
