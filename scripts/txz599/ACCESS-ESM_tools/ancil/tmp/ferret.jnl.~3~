use co2_new.nc
use co2_modified_new.nc
shade co2emiss[d=2,l=1]
show d
plot co2emiss[d=1ml=1,i=@sum,j=@sum]
plot co2emiss[d=1m,i=@sum,j=@sum]
plot co2emiss[d=1,i=@sum,j=@sum]
plot co2emiss[d=2,i=@sum,j=@sum]
plot co2emiss[d=2,i=@ave,j=@ave]
plot co2emiss[d=1,i=@ave,j=@ave]
exit
