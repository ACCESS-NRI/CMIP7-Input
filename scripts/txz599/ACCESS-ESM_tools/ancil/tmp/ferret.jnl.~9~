use co2_10pgc_constant.nc
show d
shade co2emiss[l=1]
list co2emiss[i=@sum,j=@sum,l=1]
list co2emiss[i=@sum,j=@sum,l=1]*60*60*24*365.25
list co2emiss[i=@sum,j=@sum,l=2]*60*60*24*365.25
list co2emiss[i=@ave,j=@ave,l=2]*60*60*24*365.25
list co2emiss[i=@ave,j=@ave,l=2]*60*60*24*365.25*510e12
list co2emiss[i=@ave,j=@ave,l=2]*60*60*24*365.25*510e12/3.67/1e12
use co2_10pgc_NH.nc
list co2emiss[i=@ave,j=@ave,l=2,d=1]*60*60*24*365.25*510e12/3.67/1e12
list co2emiss[i=@ave,j=@ave,l=2,d=2]*60*60*24*365.25*510e12/3.67/1e12
list co2emiss[i=@ave,j=@ave,l=2,d=2]*60*60*24*365.25*510e12/3.67/1e12*2
shade co2emiss[l=1,d=2]
use co2_10pgc_NH.nc
show d
list co2emiss[i=@ave,j=@ave,l=2,d=2]*60*60*24*365.25*510e12/3.67/1e12*2
exit
